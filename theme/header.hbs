<script>
(()=>{const e=localStorage.getItem("mdbook-theme");
if(!e){document.querySelector('meta[name="theme-color"]').content=window.getComputedStyle(document.body).backgroundColor;return;}
if(e=="{{ default_theme }}")return;
setTimeout((()=>{document.querySelector('meta[name="theme-color"]').content=window.getComputedStyle(document.body).backgroundColor}),1);
const o=document.querySelector("html");o.classList.remove("{{ default_theme }}"),o.classList.add(e)})();
</script>

