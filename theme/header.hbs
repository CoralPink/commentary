<script>
(()=>{let e=localStorage.getItem("mdbook-theme");
e||(e=matchMedia("(prefers-color-scheme: dark)").matches?"{{preferred_dark_theme}}":"{{default_theme}}"),
document.querySelector("html").classList.add(e),
setTimeout((()=>{document.querySelector('meta[name="theme-color"]').content=window.getComputedStyle(document.body).backgroundColor,
document.getElementById(e).classList.add("theme-selected")}),1)})();
</script>

<script>
(async()=>{if("serviceWorker"in navigator)
try{await navigator.serviceWorker.register("{{path_to_root}}serviceworker.js",{scope:"/commentary/"})}
catch(r){console.error(`Registration failed with ${r}`)}})();
</script>

